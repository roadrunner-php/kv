        "chat": "https://discord.gg/V6EK4he"

    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/roadrunner-server"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "psr/simple-cache": "3",
        "roadrunner-php/roadrunner-api-dto": "^1.0",
        "spiral/roadrunner": "^2023.1",
        "spiral/goridge": "^4.0",
        "google/protobuf": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\RoadRunner\\KeyValue\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "roave/security-advisories": "dev-master",
        "vimeo/psalm": ">=5.8"
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\RoadRunner\\KeyValue\\Tests\\": "tests"
        }
    },
    "scripts": {
        "tests": "phpunit",
        "psalm": "psalm --no-cache"
    },
    "suggest": {
        "ext-igbinary": "(>3.1.6) Igbinary serailizer support",
        "ext-sodium": "Sodium serailizer support"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
